{
    "name": "addwiki/addwiki-cli",
    "type": "library",
    "description": "A MediaWiki, Wikibase and Wikimedia CLI application",
    "keywords": ["Mediawiki", "Wikimedia", "CLI"],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Addshore"
        }
    ],
    "scripts": {
        "lint": "parallel-lint . --exclude vendor",
        "phpcs": "phpcs -ps",
        "fix": "phpcbf",
        "test": [
            "@lint",
            "@phpcs"
        ]
    },
    "require": {
        "php": ">=7.3",
        "addwiki/mediawiki-commands": "^3.0",
        "addwiki/wikimedia-commands": "^3.0",
        "symfony/console": "~4.0",
        "symfony/yaml": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "mediawiki/mediawiki-codesniffer": "~35.0"
    },
    "autoload": {
        "psr-4": {
            "Addwiki\\Cli\\": "src/"
        }
    },
    "bin": [
        "aww",
        "awwd"
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "3.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
