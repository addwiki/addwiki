{
    "name": "addwiki/wikimedia",
    "type": "library",
    "description": "WIP library with Wikimedia specific code for addwiki",
    "keywords": ["Wikimedia"],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Addshore"
        }
    ],
    "scripts": {
        "lint": "parallel-lint . --exclude vendor",
        "phpcs": "phpcs -ps",
        "fix": "phpcbf",
        "test": [
            "@lint",
            "@phpcs"
        ]
    },
    "require": {
        "php": "^7.2",
        "addwiki/mediawiki-api": "^2.8",
        "addwiki/mediawiki-api-base": "^2.8",
        "addwiki/wikibase-api": "^2.8",
        "data-values/common": "~0.4.0",
        "data-values/data-values": "~2.0",
        "data-values/geo": "~4.0",
        "data-values/number": "~0.10.0",
        "data-values/serialization": "~1.0",
        "data-values/time": "~1.0",
        "wikibase/data-model": "~9.2|~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "mediawiki/mediawiki-codesniffer": "~35.0"
    },
    "autoload": {
        "psr-4": {
            "Wikimedia\\Api\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.8-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
