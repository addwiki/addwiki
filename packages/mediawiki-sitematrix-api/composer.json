{
    "name": "addwiki/mediawiki-sitematrix-api",
    "type": "library",
    "description": "MediaWiki API library for the Sitematrix extension",
    "keywords": ["Mediawiki", "API"],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Addshore"
        }
    ],
    "require": {
        "addwiki/mediawiki-api-base": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~9"
    },
    "autoload": {
        "psr-4": {
            "Mediawiki\\Sitematrix\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.7-dev",
            "dev-master": "2.7-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
