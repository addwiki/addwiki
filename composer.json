{
    "autoload": {
        "files": [
            "packages/mediawiki-commands/addCommands.php",
            "packages/wikibase-commands/addCommands.php",
            "packages/wikimedia-commands/addCommands.php"
        ],
        "psr-4": {
            "Addwiki\\": "packages/addwiki-cli/src/",
            "Addwiki\\Commands\\Mediawiki\\": "packages/mediawiki-commands/src/",
            "Addwiki\\Commands\\Wikibase\\": "packages/wikibase-commands/src/",
            "Addwiki\\Commands\\Wikimedia\\": "packages/wikimedia-commands/src/",
            "Addwiki\\Topics\\Covid19\\": "packages-dev/topic-covid-19/src/",
            "Mediawiki\\Api\\": [
                "packages/mediawiki-api-base/src/",
                "packages/mediawiki-api/src/"
            ],
            "Mediawiki\\DataModel\\": "packages/mediawiki-datamodel/src/",
            "Mediawiki\\Db\\": "packages/mediawiki-db/src/",
            "Mediawiki\\Flow\\": "packages/mediawiki-flow-api/src/",
            "Mediawiki\\Sitematrix\\": "packages/mediawiki-sitematrix-api/src/",
            "Wikibase\\": "packages/wikibase-api/src/",
            "Wikimedia\\Api\\": "packages/wikimedia/src/"
        }
    },
    "require": {
        "benestar/asparagus": "~0.4",
        "data-values/common": "~0.4.0",
        "data-values/data-values": "~2.0",
        "data-values/geo": "~4.0",
        "data-values/interfaces": "~0.2.0 || ~0.1.5",
        "data-values/number": "~0.10.0",
        "data-values/serialization": "~1.0",
        "data-values/time": "~1.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "~6.0",
        "guzzlehttp/promises": "~1.0",
        "lichtner/fluentpdo": "1.0.0",
        "linclark/microdata-php": "~2.0",
        "php": "^7.2",
        "psr/log": "~1.0",
        "serialization/serialization": "~3.2 || ~4.0",
        "smalot/pdfparser": "0.14.0",
        "symfony/console": "~4.0",
        "symfony/yaml": "~4.0",
        "wikibase/data-model": "~8.0",
        "wikibase/data-model-serialization": "~2.0",
        "wikibase/data-model-services": "~3.0"
    },
    "name": "addwiki/addwiki",
    "autoload-dev": {
        "files": [
            "packages/mediawiki-api-base/tests/Integration/TestEnvironment.php"
        ],
        "psr-4": {
            "Mediawiki\\Api\\Test\\": [
                "packages/mediawiki-api/tests/integration",
                "packages/mediawiki-api/tests/unit"
            ],
            "Mediawiki\\DataModel\\Test\\": "packages/mediawiki-datamodel/tests/"
        }
    },
    "replace": {
        "addwiki/addwiki-cli": "self.version",
        "addwiki/mediawiki-api": "self.version",
        "addwiki/mediawiki-api-base": "self.version",
        "addwiki/mediawiki-commands": "self.version",
        "addwiki/mediawiki-datamodel": "self.version",
        "addwiki/mediawiki-db": "self.version",
        "addwiki/mediawiki-flow-api": "self.version",
        "addwiki/mediawiki-sitematrix-api": "self.version",
        "addwiki/topic-covid-19": "self.version",
        "addwiki/wikibase-api": "self.version",
        "addwiki/wikibase-commands": "self.version",
        "addwiki/wikimedia": "self.version",
        "addwiki/wikimedia-commands": "self.version"
    },
    "type": "project",
    "description": "Addwiki Monorepo",
    "keywords": ["Mediawiki"],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Addshore"
        },
        {
            "name": "Lucie-Aim√©e Kaffee"
        }
    ],
    "require-dev": {
        "data-values/validators": "~0.1.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "mediawiki/mediawiki-codesniffer": "^13.0",
        "monolog/monolog": "^1.23",
        "phpunit/phpunit": "~8",
        "symplify/monorepo-builder": "^9.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/*"
        },
        {
            "type": "path",
            "url": "packages-dev/*"
        }
    ],
    "scripts": {
        "lint": "bin/run-all vendor/bin/parallel-lint DIR --exclude DIR/vendor",
        "phpcs": "bin/run-all vendor/bin/phpcs -ps DIR",
        "fix": "bin/run-all vendor/bin/phpcbf DIR",
        "phpunit": "bin/run-all vendor/bin/phpunit DIR",
        "phpunit-unit": "bin/run-all vendor/bin/phpunit --testsuite unit DIR",
        "phpunit-integration": "bin/run-all vendor/bin/phpunit --testsuite integration DIR",
        "test": [
            "@lint",
            "@phpcs",
            "@phpunit"
        ],
        "aww": "aww",
        "awwd": "echo Please run ./vendor/bin/awwd directly",
        "post-autoload-dump": "bin/vendor-autoload-all"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
